cmake_minimum_required(VERSION 2.8.3)
project(ps3)

find_package(catkin_simple REQUIRED)

catkin_simple()

# Executables
cs_add_executable(my_example_ros_service src/example_ros_service.cpp)
cs_add_executable(my_example_ros_client src/example_ros_client.cpp)
cs_add_executable(my_path_service src/path_service.cpp)
cs_add_executable(my_path_service2 src/path_service2.cpp)
#cs_add_executable(path_service_soln src/path_service_soln.cpp)
cs_add_executable(my_path_client src/path_client.cpp)
cs_install()
cs_export()
    
